                $stateProvider
                      //银行管理页
                      .state('home.index.bank_page',{
                        abstract: true,
                        url: '/bank_page',
                        templateUrl: 'views/client_manage/bank_page.html',
                        controller: 'bankListCtrl'
                      })
                      .state('home.index.bank_page.list', {
                          url: '/list'
                      })
                    //以下为银行管理DEMO页面
                    .state('home.index.credit_index', {
                        abstract: true,
